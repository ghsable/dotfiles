[user]
	name = 'ghsable'
	email = 'sn.sable005@gmail.com'
[core]
	editor = vim
[alias]
	deleted = log --diff-filter=D --summary
	thisbranch = branch --contains=HEAD
	ignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
	test = "!et() { echo $1 ;}; et"
[color]
	ui = 1
[branch]
	autosetuprebase = always
[credential]
	helper = cache --timeout=3600
[init]
	defaultBranch = main
